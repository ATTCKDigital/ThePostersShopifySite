<div class="container">
	<div class="row">
		<div class="col-sm-12 text-center">
			<h1 class="shadow-header section-header">The Artists</h1>
			<ul class="artist-list">
				{% for vendor in shop.vendors %}
					{% for collection in collections %}
						{% if collection.title == vendor %}
						  <li class="artist" style="background-image: url({{ collection | img_url: 'large' }})"><a href="/collections/{{ vendor | handleize }}">{{ vendor }}</a></li>
						{% endif %}
					{% endfor %}
				{% endfor %}
				<li id="join-posters"><a href="/pages/submissions"><img src="{{ 'joinposters.png' | asset_url }}" /></a></li>
			</ul>
		</div>
	</div>
</div>

<div class="archive-border" style="background-image: url({{ 'archiveborder.png' | asset_url }})"></div>
